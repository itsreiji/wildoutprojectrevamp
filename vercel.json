{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/inngest",
      "dest": "/api/inngest.ts"
    },
    {
      "src": "/api/inngest/(.*)",
      "dest": "/api/inngest.ts"
    },
    {
      "src": "/health",
      "dest": "/api/inngest.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/inngest.ts": {
      "memory": 1024,
      "maxDuration": 60
    }
  }
}
